{
  "name": "adamwojs/symfony-config-schema-gen",
  "license": "GPL-2.0-only",
  "keywords": [
    "symfony", "symfony-bundle", "config", "json-schema"
  ],
  "type": "symfony-bundle",
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/yaml": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "php-cs-fixer/shim": "^3.86"
  },
  "autoload": {
    "psr-4": {
      "AdamWojs\\SymfonyConfigGenBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AdamWojs\\SymfonyConfigGenBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fix-cs": "php-cs-fixer fix -v --show-progress=bar",
    "check-cs": "php-cs-fixer fix --dry-run -v --show-progress=bar",
    "unit-tests": "phpunit -c phpunit.xml.dist",
    "test": [
      "@unit-tests"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}
